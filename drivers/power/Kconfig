config MMI_QC3P_TURBO_CHARGER
	bool "Enable MMI QC3P Turbo Charger"
	default n
	help
	  Say Y here if you have a MMI QC3P turbo charger compatible device connected.
	  If unsure, say N.

config MMI_QC3P_WT6670_DETECTED
	bool "Enable MMI QC3P WT6670 detection"
	default n
	help
	  Say Y here to enable MMI QC3P WT6670 detection.
	  If unsure, say N.

config MMI_QC3P_Z350_DETECTED
	bool "Enable MMI QC3P Z350 detection"
	default n
	help
	  Say Y here to enable MMI QC3P Z350 detection.
	  If unsure, say N.

config MMI_EXT_CHG_LED
	bool "Enable MMI external charger LED"
	default n
	help
	  Say Y here if you have a MMI external charger LED.
	  If unsure, say N.

